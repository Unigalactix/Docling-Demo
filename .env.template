# Copy this file to `.env` and fill in your values.
# Do NOT commit your filled `.env` to source control.

# Primary provider (OpenAI)
OPENAI_API_KEY=sk-your-openai-key
# Optional: default model for OpenAI chat/completions
OPENAI_MODEL=gpt-4o-mini

# Fallback provider (OpenRouter)
OPENROUTER_API_KEY=sk-or-your-openrouter-key
# Optional: configure OpenRouter base URL if you use their endpoint directly
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
# Optional: default model for OpenRouter
OPENROUTER_MODEL=openrouter/auto
# Optional but recommended per OpenRouter docs
OPENROUTER_HTTP_REFERER=
OPENROUTER_X_TITLE=Docling Demo

# Hugging Face Hub settings for Windows (avoid hardlink/symlink warnings)
HF_HUB_DISABLE_SYMLINKS_WARNING=1
HF_HUB_DISABLE_HARDLINKS=1
